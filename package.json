		"@testing-library/svelte": "^4.1.0",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.21.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^22.1.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.10",
		"svelte-check": "^3.6.6",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^4.5.3",
		"vitest": "^2.0.1"
