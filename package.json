		"@typescript-eslint/parser": "^8.24.0",
		"eslint": "^9.19.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^25.0.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"svelte": "^4.2.19",
		"svelte-check": "^4.1.1",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"vite": "^5.4.18",
